<div class="container">
<article id="<%= @question.id %>">
  <form method="question" action='/questions/<%= @question.id %>/vote' class="inline">
    <button type="submit" name="submit_param" value="submit_value" class="upvote-button"></button>
  </form>
  <h2><a href='/questions/<%= @question.id %>'><%= @question.title %></a></h2>
  <p>
    <span class='points'><%= @question.point_count %></span>
    <span class='username'><%= @question.username %></span>
    <span class='timestamp'><%= @question.created_at %></span>
    <a class="delete" href='/@questions/<%= @question.id %>'></a>
  </p>
</article>
</div>
